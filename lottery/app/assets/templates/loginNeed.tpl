<div class="">
	<div class="">
		<div class="">
			<div class="panel panel-default">
				<div class="panel-heading">
					<h3 class="panel-title text-center">完善信息</h3>
				</div>
				<div class="panel-body">
					<form id="registerForm">
						<div class="form-group">
							<label>公司名称：</label>
							<input type="text" name="company_name" class="form-control input-sm" placeholder="公司全称"/>
						</div>
						<div class="form-group">
							<label>公司地址：</label>
							<input type="text" name="company_addr" class="form-control input-sm" placeholder="公司地址"/>
						</div>
						<div class="form-group">
							<label>网站地址：</label>
							<input type="text" name="company_website" class="form-control input-sm" placeholder="网站地址"/>
						</div>
						<div class="form-group">
							<label>联系人姓名：</label>
							<input type="text" name="company_contact_name" class="form-control input-sm" placeholder="联系人姓名"/>
						</div>
						<div class="form-group">
							<label>联系人电话：</label>
							<input type="text" name="company_contact_phone" class="form-control input-sm" placeholder="联系人电话"/>
						</div>
                    	<br>
						<div class="form-group">
							<input type="submit" value="提交" class="btn btn-primary btn-block"/>
						</div>
					</form>					
				</div>
			</div>
		</div>
	</div>
</div>